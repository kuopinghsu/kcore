if ARCH_CHIP_KCORE

comment "KCORE Configuration Options"

config KCORE_UART0
	bool "UART0"
	default n
	select UART0_SERIALDRIVER
	select ARCH_HAVE_SERIAL_TERMIOS

endif # ARCH_CHIP_KCORE
